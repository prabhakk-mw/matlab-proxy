{
    "name": "Only build MATLAB Proxy",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".",
        "args": {
            "PYTHON_VERSION" : "3.11",
            "NVM_VERSION" : "0.40.3",
            "NODE_VERSION" : "24"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python"
            ],
            "settings": {
            }
        }
    },
    "postCreateCommand" : "rm -rf gui/package-lock.json && uv venv --clear --python 3.11 && . .venv/bin/activate && uv pip install . --cache-dir . --verbose"
}
